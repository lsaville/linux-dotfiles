#!/bin/bash

xrandr --output HDMI-1 --brightness 0.5
xrandr --output HDMI-2 --brightness 0.5
sleep 0.5
xrandr --output HDMI-1 --brightness 1
xrandr --output HDMI-2 --brightness 1
sleep 0.5
xrandr --output HDMI-1 --brightness 0.5
xrandr --output HDMI-2 --brightness 0.5
sleep 0.5
xrandr --output HDMI-1 --brightness 1
xrandr --output HDMI-2 --brightness 1
sleep 0.5
xrandr --output HDMI-1 --brightness 0.5
xrandr --output HDMI-2 --brightness 0.5
sleep 0.5
xrandr --output HDMI-1 --brightness 1
xrandr --output HDMI-2 --brightness 1
