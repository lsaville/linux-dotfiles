#!/usr/bin/expect -f

#QA1
spawn cap qa1 puma:restart

expect "Please enter whether you are deploying from a branch (y/n): "
send "y\r"

expect "Please enter whether you are on location at AFC? (y/n): "
send "y\r"

expect eof

